import pandas as pd

data = {
    'raw_text': [
        "Клиент Асылхан Нурсултанов, ИИН: 920315301847, подал заявку на кредит в размере 2,500,000 тенге. Его контактный номер: +7 (777) 123-45-67.",
        "Другой клиент, ИИН: 850101123456, подал заявку.Его контактный номер: +7 (777) 222-45-67.",
        "Текст без ИИН для проверки."
    ]
}

df = pd.DataFrame(data)

df['iin'] = df['raw_text'].str.extract(r'ИИН:\s*(\d{12})')

print(df)
